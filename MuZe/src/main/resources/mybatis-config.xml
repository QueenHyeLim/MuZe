		<typeAlias type="com.kh.muze.bookmark.model.vo.Bookmark" alias="bookmark" />
		<typeAlias type="com.kh.muze.board.model.vo.Deal" alias="deal"/>
		<typeAlias type="com.kh.muze.board.model.vo.Like" alias="like"/>
		<typeAlias type="com.kh.muze.rank.model.vo.Rank" alias="rank"/>
	</typeAliases>
	 
	<mappers>
    	<mapper resource="/mapper/calendar-mapper.xml"/>
		<mapper resource="/mapper/member-mapper.xml" />
		<mapper resource="/mapper/reservation-mapper.xml" />
		<mapper resource="/mapper/seat-mapper.xml" />
		<mapper resource="/mapper/board-mapper.xml"/>
		<mapper resource="/mapper/bookmark-mapper.xml"/>
		<mapper resource="/mapper/rank-mapper.xml" />
	</mappers>
	
</configuration>